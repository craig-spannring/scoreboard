      "args": 
      [
         "--bogus"
      ],  
      "projectName": "crg-scoreboard"
    },
    {
      "type": "java",
      "name": "scoreboard --metrics",
      "request": "launch",
      "mainClass": "com.carolinarollergirls.scoreboard.Main",
